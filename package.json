{
  "name": "vite-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx,js,json,vue}\" \"*.config.{ts,js}\" --fix",
    "prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,ts,css,scss,vue,html,md}\"",
    "lint:style": "stylelint \"**/*.{vue,css,scss,sass}\" \"!dist/**/*\""
  },
  "dependencies": {
    "@vueuse/core": "^6.7.3",
    "axios": "^0.23.0",
    "pinia": "^2.0.0-rc.14",
    "vant": "^3.2.5",
    "vconsole": "^3.9.3",
    "vue": "^3.2.16",
    "vue-i18n": "^9.2.0-beta.14",
    "vue-router": "4",
    "vue-types": "^4.1.1"
  },
  "devDependencies": {
    "@amatlash/vite-plugin-stylelint": "^1.1.1",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@intlify/vite-plugin-vue-i18n": "^2.4.0",
    "@types/node": "^16.11.2",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vitue/export-build-info": "^1.0.0",
    "@w6s/eslint-config": "^2.2.0",
    "@w6s/stylelint-config": "^1.5.0",
    "eslint": "^7.19.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "sass": "^1.43.3",
    "stylelint": "^13.9.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-style-import": "^1.2.1",
    "vite-plugin-vconsole": "^1.1.0",
    "vite-plugin-windicss": "^1.4.12",
    "vite-svg-loader": "^2.2.0",
    "vue-tsc": "^0.3.0",
    "windicss": "^3.1.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "npm run lint",
      "npm run prettier",
      "npm run lint:style"
    ]
  },
  "engines": {
    "node": ">=12"
  }
}
